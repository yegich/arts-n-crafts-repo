package yega.alogorithms.randomizecontraction;

public final class Edge {

	private String id;
	
	public String getId() {
		return id;
	}


	public static class Builder{
		private Edge edge;
		
		public Builder() {
			edge = new Edge();
		}
		
		public Edge build() {
			return edge;
		}
		

		public Builder id(String id) {
			edge.id = id;
			return this;
		}
	}


	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		return result;
	}


	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Edge other = (Edge) obj;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		return true;
	}


	@Override
	public String toString() {
		return "Edge [id=" + id + "]";
	}
	
}
