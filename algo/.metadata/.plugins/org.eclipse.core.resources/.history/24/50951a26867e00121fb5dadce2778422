package yega.alogorithms.randomizecontraction;


public class Edge {

	private Double id;
	private Vertex beginVertex;
	private Vertex endVertex;
	
	public Double getId() {
		return id;
	}
	public Vertex getBeginVertex() {
		return beginVertex;
	}
	public Vertex getEndVertex() {
		return endVertex;
	}
	
	public static class Builder{
		private Edge edge;
		
		public Builder() {
			edge = Edge();
		}
		
		public Edge build() {
			return edge;
		}
		
		public Builder id(Double id) {
			edge.id = id;
			return this;
		}
		
		public Builder vertecis(Vertex beginVertex, Vertex endVertex) {
			edge.beginVertex = beginVertex;
			edge.endVertex = endVertex;
			return this;
		}
	}
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ ((beginVertex == null) ? 0 : beginVertex.hashCode());
		result = prime * result
				+ ((endVertex == null) ? 0 : endVertex.hashCode());
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Edge other = (Edge) obj;
		if (beginVertex == null) {
			if (other.beginVertex != null)
				return false;
		} else if (!beginVertex.equals(other.beginVertex))
			return false;
		if (endVertex == null) {
			if (other.endVertex != null)
				return false;
		} else if (!endVertex.equals(other.endVertex))
			return false;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		return true;
	}
	
	
}
